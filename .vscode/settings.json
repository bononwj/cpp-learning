{
  "cmake.configureOnOpen": true,
  "cmake.buildDirectory": "${workspaceFolder}/build",
  "cmake.generator": "Unix Makefiles",
  "cmake.preferredGenerators": ["Unix Makefiles", "Ninja", "Xcode"],
  "cmake.debugConfig": {
    "stopAtEntry": true,
    "cwd": "${workspaceFolder}"
  },
  "cmake.buildConfig": "Debug",
  "cmake.parallelJobs": 4,
  "cmake.installPrefix": "${workspaceFolder}/install",
  "cmake.cmakePath": "/usr/local/bin/cmake",
  "C_Cpp.default.compilerPath": "/usr/bin/clang++",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.intelliSenseMode": "macos-clang-x64",
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/**",
    "/usr/local/include",
    "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
    "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/clang/*/include",
    "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include"
  ],
  "C_Cpp.default.defines": ["_DEBUG"],
  "C_Cpp.default.browse.path": [
    "${workspaceFolder}/**",
    "/usr/local/include",
    "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
    "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/clang/*/include",
    "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include"
  ],
  "files.associations": {
    "*.dbclient-js": "javascript",
    "*.ejs": "html",
    "*.wpy": "vue",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.kt": "gradle-kotlin-dsl",
    "__node_handle": "cpp",
    "__tree": "cpp",
    "__verbose_abort": "cpp",
    "array": "cpp",
    "cstdlib": "cpp",
    "memory": "cpp",
    "initializer_list": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "map": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "tuple": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "algorithm": "cpp",
    "cmath": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstring": "cpp",
    "cwchar": "cpp",
    "string_view": "cpp",
    "__bit_reference": "cpp",
    "__hash_table": "cpp",
    "__locale": "cpp",
    "__split_buffer": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "complex": "cpp",
    "cstdarg": "cpp",
    "ctime": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "execution": "cpp",
    "locale": "cpp",
    "mutex": "cpp",
    "optional": "cpp",
    "print": "cpp",
    "queue": "cpp",
    "ratio": "cpp",
    "sstream": "cpp",
    "stack": "cpp",
    "variant": "cpp",
    "regex": "cpp",
    "__threading_support": "cpp",
    "cstddef": "cpp",
    "__config": "cpp"
  }
}
